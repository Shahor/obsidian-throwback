{
	"name": "obsidian-throwback",
	"type": "module",
	"version": "1.0.0",
	"description": "This is a throwback to the past plugin. It'll help be aware of notes written the same day in the past.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "node --loader tsx --watch --test src/**/*.test.ts"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@faker-js/faker": "^7.6.0",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"eslint": "^8.38.0",
		"obsidian": "latest",
		"ts-node": "^10.9.1",
		"tsc": "^2.0.4",
		"tslib": "2.4.0",
		"tsx": "^3.12.6",
		"typescript": "4.7.4"
	},
	"dependencies": {}
}
